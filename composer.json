{
    "name": "rspaeth/slim-twig",
    "description": "A collection of Slim 4 Middleware & Twig 3 Extensions.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Spaeth",
            "email": "rspaeth@mvqn.net"
        }
    ],
    "require": {
        "php": "~7.3",
        "ext-json": "*",
        "slim/slim": "^4.4",
        "slim/psr7": "^1.0",
        "php-di/php-di": "^6.0",
        "twig/twig": "^3.0",
        "slim/twig-view": "^3.0",
        "rspaeth/slim": "*",
        "rspaeth/twig": "*",
        "doctrine/annotations": "^1.8",
        "php-di/slim-bridge": "^3.0"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "rspaeth\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        { "type": "path", "url": "../*"}
    ]
}
